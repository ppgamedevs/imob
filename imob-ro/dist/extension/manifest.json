{
  "manifest_version": 3,
  "name": "YOUR_DOMAIN Extractor",
  "description": "Analizează un link - injectează extractor și trimite datele către YOUR_DOMAIN",
  "version": "0.1.0",
  "action": {
    "default_title": "Analizează pe YOUR_DOMAIN",
    "default_icon": "icon.svg"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost:3000/*", "https://YOUR_DOMAIN/*"],
  "background": {
    "service_worker": "background.js"
  }
}
